def exibir_mensagem():
    print("Ola mundo")

def exibir_mensagem_2(nome):
    print(f"Seja bem vindo {nome}!")

def exibir_mensagem_3(nome="Antonio"):
    print(f"Seja bem vindo {nome}") 
    
exibir_mensagem()
exibir_mensagem_2(nome = "Guilherme")
exibir_mensagem_3()
exibir_mensagem_3(nome = "Rian")
